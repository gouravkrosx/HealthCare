<%- include("partials/header") -%>

  <div class="jumbotron text-center" style="margin-bottom:0">
    <h1>Welcome Dr. <span class="name">
        <%=user.name%>
      </span> </h1>
    <p>This will render the speacialization of the doctor</p>
  </div>

  <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
    <a class="navbar-brand" href="/"><img class="doc-logo" src="/assets/RGSlogo.png" alt="logo"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="collapsibleNavbar">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link" href="/Ddashboard/DmyAppointments">My appointments</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">My patients</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Covid Update</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/users/logout"><button class="btn btn-primary">Log out</button></a>
        </li>
      </ul>
    </div>
  </nav>

  <div class="container" style="margin-top:30px">
    <div class="row">
      <div class="col-sm-4">
        <h2>About Me</h2>
        
        <h5>Photo of me:</h5>
        <div class="fakeimg">
          <card>
            <img class="Dprofile"
              src="data:image/<%=user.photo.contentType%>;base64,<%=user.photo.data.toString('base64')%>" width="340"
              height="200" alt="">
          </card>
        </div>

        <h3>Dr. <span class="name">
            <%=user.name%>
          </span>
        </h3>

        <ul class="nav nav-pills flex-column">
          <li class="nav-item">
            <a class="nav-link active" href="/myRecords">My Records</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">View My Prescriptions</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Change Appointment Timings</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/Ddashboard/DeditProfile">Edit Profile</a>
          </li>
        </ul>
        <hr class="d-sm-none">
      </div>
      <div class="col-sm-8">
        <h2>Qualification</h2>
        <h5>Speciality: <%= user.speciality %>
        </h5>
        <div class="fakeimg"><img
            src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTcAaypKSIqeCM7qT6KEtquljx8YKxfXpt-_w&usqp=CAU"
            alt=""></div>
        <p>Sunt in culpa qui officia deserunt mollit anim id est laborum consectetur adipiscing elit, sed do
          eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud
          exercitation ullamco.</p>
        <br>
        <h2>Experience</h2>
        <h5>
          <%= user.yearsOfPractice %> Years of Experience
        </h5>
        <div class="fakeimg"><img
            src="data:image/jpeg;base64,"
            alt=""></div>
        <p>Sunt in culpa qui officia deserunt mollit anim id est laborum consectetur adipiscing elit, sed do
          eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud
          exercitation ullamco.</p>
      </div>
    </div>
  </div>

  <div class="container-fluid pb-0 mb-0 justify-content-center text-light ">
    <footer>
      <div class="row my-5 justify-content-center py-5">
        <div class="col-11">
          <div class="row ">
            <div class="col-xl-8 col-md-4 col-sm-4 col-12 my-auto mx-auto a">
              <h3 class="text-muted mb-md-0 mb-5 bold-text">RGS Health Care</h3>
            </div>
            <div class="col-xl-2 col-md-4 col-sm-4 col-12">
              <h6 class="mb-3 mb-lg-4 bold-text "><b>MENU</b></h6>
              <ul class="list-unstyled">
                <li><a href="#">My Blogs</a> </li>
                <li> <a href="#">My Portfolio</a></li>
              </ul>
            </div>
            <div class="col-xl-2 col-md-4 col-sm-4 col-12">
              <h6 class="mb-3 mb-lg-4 text-muted bold-text mt-sm-0 mt-5"><b>ADDRESS</b></h6>
              <p class="mb-1">605, Anmol Apartments</p>
              <p>Dwarka , Delhi</p>
            </div>
          </div>
          <div class="row ">
            <div class="col-xl-8 col-md-4 col-sm-4 col-auto my-md-0 mt-5 order-sm-1 order-3 align-self-end">
              <p class="social text-muted mb-0 pb-0 bold-text"> <span class="mx-2"><i class="fa fa-facebook"
                    aria-hidden="true"></i></span> <span class="mx-2"><i class="fa fa-linkedin-square"
                    aria-hidden="true"></i></span> <span class="mx-2"><i class="fa fa-twitter"
                    aria-hidden="true"></i></span> <span class="mx-2"><i class="fa fa-instagram"
                    aria-hidden="true"></i></span> </p><small class="rights"><span>&#174;</span> RGS Health Care All
                Rights Reserved.</small>
              <div class="Date">
                <p>
                  <%= new Intl.DateTimeFormat('en-GB', { year: 'numeric' , month: 'long' , day: '2-digit' }).format(new
                    Date()) %>
                </p>
                <div id="clock"></div>
              </div>
            </div>
            <div class="col-xl-2 col-md-4 col-sm-4 col-auto order-1 align-self-end ">
              <h6 class="mt-55 mt-2 text-muted bold-text"><b>
                  Dr. <span class="name">
                    <%=user.name%>
                  </span>
                </b></h6><small> <span><i class="fa fa-envelope" aria-hidden="true"></i></span>
                rocksarthak@gmail.com</small>
            </div>
            <div class="col-xl-2 col-md-4 col-sm-4 col-auto order-2 align-self-end mt-3 ">
              <h6 class="text-muted bold-text"><b> Office mail adress</b></h6><small><span><i class="fa fa-envelope"
                    aria-hidden="true"></i></span>
                cooloffice@gmail.com</small>
            </div>
          </div>
        </div>
      </div>
    </footer>
  </div>



  <%- include("partials/footer") -%>